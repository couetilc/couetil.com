#!/usr/bin/env bash

set -e

container_id="$(docker ps --filter ancestor="$docker_image" --format "{{.ID}}")"

if [ -n "$container_id" ]; then
	echo
	echo "Warning: container is already running"
	echo
	docker ps --filter ancestor="$docker_image"
else
	docker run \
	 -d \
	 -p 9000:8080 \
	 --platform linux/arm64 \
	 "$docker_image" \
	 ./bootstrap
fi
